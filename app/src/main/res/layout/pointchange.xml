<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent" android:layout_height="match_parent" style="@r$style/sub_page_bg">
    <RelativeLayout android:id="@+id/tabbar" android:layout_width="match_parent" style="@r$style/tabbar_height">
        <ImageView android:id="@+id/topbg" android:layout_width="match_parent" android:layout_height="match_parent" style="@r$style/top_background"/>
        <ImageButton android:id="@+id/top_back" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginLeft="10dp" android:layout_marginRight="2dp" android:src="@drawable/btn_back" android:layout_alignTop="@+id/topbg" android:layout_alignBottom="@+id/topbg" style="@r$style/img_basic"/>
        <ImageButton android:id="@+id/top_refresh" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginLeft="2dp" android:layout_marginRight="10dp" android:src="@drawable/btn_refresh" android:layout_alignTop="@+id/topbg" android:layout_alignBottom="@+id/topbg" android:layout_alignParentRight="true" style="@r$style/img_basic"/>
        <TextView android:id="@+id/top_title" android:layout_width="match_parent" android:layout_height="match_parent" android:text="@string/text_pointrequest" android:layout_toLeftOf="@+id/top_refresh" android:layout_toRightOf="@+id/top_back" android:layout_alignTop="@+id/topbg" android:layout_alignBottom="@+id/topbg" style="@r$style/tabbar_title_size"/>
    </RelativeLayout>
    <ScrollView android:id="@+id/scrollview" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/tabbar">
        <RelativeLayout android:padding="10dp" android:layout_width="match_parent" android:layout_height="wrap_content">
            <RelativeLayout android:layout_gravity="center_horizontal" android:id="@+id/frame1" android:layout_width="match_parent" android:layout_height="wrap_content" style="@r$style/content_bg">
                <LinearLayout android:gravity="center_vertical" android:orientation="vertical" android:id="@+id/frame1_1" android:paddingLeft="10dp" android:paddingTop="5dp" android:paddingRight="10dp" android:paddingBottom="5dp" android:layout_width="match_parent" android:layout_height="330dp">
                    <RelativeLayout android:layout_gravity="center_vertical" android:id="@+id/frame4_1" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1">
                        <ImageView android:id="@+id/icon_1" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginRight="5dp" android:src="@drawable/icon_round_gray" style="@r$style/img_basic"/>
                        <TextView android:id="@+id/info9" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginRight="5dp" android:text="@string/text_changerealpoint" android:layout_toRightOf="@+id/icon_1" style="@r$style/text17sp_left_black"/>
                        <TextView android:id="@+id/usemile" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_toRightOf="@+id/info9" style="@r$style/text17sp_right_black"/>
                    </RelativeLayout>
                    <RelativeLayout android:gravity="center_vertical" android:id="@+id/frame4_8" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1">
                        <TextView android:textSize="13sp" android:textColor="#999999" android:gravity="left" android:id="@+id/info10" android:paddingLeft="10dp" android:paddingBottom="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/text_changemileinfo"/>
                        <View android:id="@+id/underline3" android:layout_width="match_parent" android:layout_below="@+id/info10" style="@r$style/unline_bg"/>
                    </RelativeLayout>
                    <RelativeLayout android:orientation="horizontal" android:id="@+id/frame4_2" android:paddingBottom="5dp" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1">
                        <ImageView android:id="@+id/icon_2" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginRight="5dp" android:src="@drawable/icon_round_gray" style="@r$style/img_basic"/>
                        <TextView android:id="@+id/info2" android:layout_width="90dp" android:layout_height="match_parent" android:text="@string/text_changepoint" android:layout_toRightOf="@+id/icon_2" style="@r$style/text17sp_left_black"/>
                        <Spinner android:id="@+id/cashspinner" android:background="@drawable/bg_spinner_grey" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginLeft="5dp" android:layout_toRightOf="@+id/info2" style="@r$style/text17sp_center_black"/>
                    </RelativeLayout>
                    <RelativeLayout android:id="@+id/frame4_3" android:paddingBottom="5dp" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1">
                        <ImageView android:id="@+id/icon_3" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginRight="5dp" android:src="@drawable/icon_round_gray" style="@r$style/img_basic"/>
                        <TextView android:id="@+id/info3" android:layout_width="90dp" android:layout_height="40dp" android:layout_marginRight="5dp" android:text="@string/text_changefee" android:layout_toRightOf="@+id/icon_3" style="@r$style/text17sp_left_black"/>
                        <TextView android:id="@+id/fee" android:layout_width="match_parent" android:layout_height="40dp" android:text="@string/text_changefeepay" android:layout_toRightOf="@+id/info3" style="@r$style/text17sp_right_black"/>
                    </RelativeLayout>
                    <RelativeLayout android:id="@+id/frame4_4" android:paddingBottom="5dp" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1">
                        <ImageView android:id="@+id/icon_4" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginRight="5dp" android:src="@drawable/icon_round_gray" style="@r$style/img_basic"/>
                        <TextView android:id="@+id/info4" android:layout_width="90dp" android:layout_height="40dp" android:text="@string/text_changebank" android:layout_toRightOf="@+id/icon_4" style="@r$style/text17sp_left_black"/>
                        <Spinner android:id="@+id/bankspinner" android:background="@drawable/bg_spinner_grey" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginLeft="5dp" android:layout_toRightOf="@+id/info4" style="@r$style/text17sp_center_black"/>
                    </RelativeLayout>
                    <RelativeLayout android:id="@+id/frame4_5" android:paddingBottom="5dp" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1">
                        <ImageView android:id="@+id/icon_5" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginRight="5dp" android:src="@drawable/icon_round_gray" style="@r$style/img_basic"/>
                        <TextView android:id="@+id/info5" android:layout_width="90dp" android:layout_height="40dp" android:layout_marginRight="5dp" android:text="@string/text_changeaccount" android:layout_toRightOf="@+id/icon_5" style="@r$style/text17sp_left_black"/>
                        <EditText android:id="@+id/accountnum" android:background="@drawable/bg_edit_grey" android:layout_width="match_parent" android:layout_height="40dp" android:text="" android:hint="@string/text_changenumber" android:singleLine="true" android:maxLength="50" android:digits="0123456789" android:layout_toRightOf="@+id/info5" android:inputType="number" style="@r$style/text14sp_center_black"/>
                    </RelativeLayout>
                    <RelativeLayout android:id="@+id/frame4_6" android:paddingBottom="5dp" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1">
                        <ImageView android:id="@+id/icon_6" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginRight="5dp" android:src="@drawable/icon_round_gray" style="@r$style/img_basic"/>
                        <TextView android:id="@+id/info6" android:layout_width="90dp" android:layout_height="40dp" android:layout_marginRight="5dp" android:text="@string/text_changeaccountnm" android:layout_toRightOf="@+id/icon_6" style="@r$style/text17sp_left_black"/>
                        <EditText android:gravity="center_vertical" android:id="@+id/accountnm" android:background="@drawable/bg_edit_grey" android:layout_width="match_parent" android:layout_height="40dp" android:singleLine="true" android:maxLength="20" android:layout_toRightOf="@+id/info6" android:inputType="text" style="@r$style/text17sp_center_black"/>
                    </RelativeLayout>
                    <RelativeLayout android:id="@+id/frame4_7" android:paddingBottom="5dp" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1">
                        <ImageView android:id="@+id/icon_7" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginRight="5dp" android:src="@drawable/icon_round_gray" style="@r$style/img_basic"/>
                        <TextView android:id="@+id/info7" android:layout_width="90dp" android:layout_height="40dp" android:layout_marginRight="5dp" android:text="@string/text_changegivedate" android:layout_toRightOf="@+id/icon_7" style="@r$style/text17sp_left_black"/>
                        <TextView android:id="@+id/givedate" android:layout_width="match_parent" android:layout_height="40dp" android:text="" android:layout_toRightOf="@+id/info7" style="@r$style/text17sp_center_black"/>
                    </RelativeLayout>
                </LinearLayout>
                <LinearLayout android:gravity="center" android:id="@+id/frame1_2" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="20dp" android:layout_marginTop="10dp" android:layout_marginRight="20dp" android:layout_marginBottom="10dp" android:layout_below="@+id/frame1_1">
                    <Button android:id="@+id/reqmilebtn" android:background="@drawable/btn_icon_green" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginRight="5dp" android:text="@string/text_request" android:layout_weight="1" style="@r$style/text20sp_center"/>
                    <Button android:id="@+id/cancelbtn" android:background="@drawable/btn_icon_grey" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginLeft="5dp" android:text="@string/text_cancel" android:layout_weight="1" style="@r$style/text20sp_center"/>
                </LinearLayout>
                <RelativeLayout android:id="@+id/frame1_3" android:padding="10dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/frame1_2">
                    <TextView android:textColor="#999999" android:id="@+id/info8" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/text_changeinfo" style="@r$style/text12sp_left_black"/>
                </RelativeLayout>
            </RelativeLayout>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>
